		<TextColor>
			<R>255</R>
			<G>255</G>
			<B>255</B>
		</TextColor>
		<ButtonDrawTemplate>
			<Normal>A_CheckBoxNormal</Normal>
			<Pressed>A_CheckBoxPressed</Pressed>
